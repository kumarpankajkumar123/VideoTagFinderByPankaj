<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register"
    android:background="#FFFFFF">

    <ImageView
        android:layout_width="17dp"
        android:layout_height="10.11dp"
        android:layout_marginTop="73.95dp"
        android:layout_marginStart="67dp"
        android:src="@drawable/backarrow"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25.32dp"
        android:layout_marginTop="67dp"
        android:layout_marginStart="315dp"
        android:src="@drawable/signinlogo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="154dp"
        android:layout_marginEnd="50dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="Create your account"
        android:textColor="#000000"
        android:textSize="26.72sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="214dp"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="Name"
        android:fontFamily="@font/poppins"
        android:textColor="#6F6F6F"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:inputType="text"
        android:hint="pankaj"
        android:textColorHint="#888888"
        android:textColor="#888888"
        android:layout_marginTop="238dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="50dp"
        android:paddingStart="5dp"
        android:background="@drawable/email_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:layout_width="45dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="297dp"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="Email"
        android:textColor="#6F6F6F"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:inputType="textEmailAddress"
        android:hint="pankaj@123"
        android:textColorHint="#888888"
        android:textColor="#888888"
        android:layout_marginTop="323dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="50dp"
        android:paddingStart="5dp"
        android:background="@drawable/email_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="380dp"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="Password"
        android:textColor="#6F6F6F"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:inputType="textPassword"
        android:hint="pankaj@123"
        android:textColorHint="#888888"
        android:textColor="#888888"
        android:layout_marginTop="403.84dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="50dp"
        android:paddingStart="5dp"
        android:background="@drawable/email_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="463dp"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="Confirm password"
        android:textColor="#6F6F6F"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:inputType="textPassword"
        android:hint="pankaj@123"
        android:textColorHint="#888888"
        android:textColor="#888888"
        android:layout_marginTop="486dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="50dp"
        android:paddingStart="5dp"
        android:background="@drawable/email_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <CheckBox
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginTop="547dp"
        android:layout_marginStart="53dp"
        android:background="@drawable/checkbox"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/condition"/>

    <TextView
        android:id="@+id/condition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="73dp"
        android:layout_marginTop="547dp"
        android:gravity="center"
        android:text="I understand the"
        android:textColor="#000000"
        android:textSize="12sp"
        android:fontFamily="@font/poppins"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="547dp"
        android:gravity="center"
        android:text="Term and Policy"
        android:textColor="#00B140"
        android:textSize="12sp"
        android:fontFamily="@font/poppins"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.button.MaterialButton
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#00B140"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sign up"
        android:elevation="4dp"
        android:textColor="#ffffff"
        android:textSize="15sp"
        app:cornerRadius="10dp"
        android:gravity="center"
        android:layout_marginTop="578dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="or sign in with"
        android:textSize="16sp"
        android:fontFamily="@font/poppins"
        android:letterSpacing="0.02"
        android:textColor="#888888"
        android:layout_marginTop="642dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <LinearLayout
        android:layout_width="86dp"
        android:layout_height="42dp"
        android:background="#F4F4F4"
        android:gravity="center"
        android:layout_marginTop="680dp"
        android:layout_marginStart="51dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="27dp"
            android:layout_height="27.54dp"
            android:background="#F4F4F4"
            android:src="@drawable/google"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="86dp"
        android:layout_height="42dp"
        android:background="#F4F4F4"
        android:gravity="center"
        android:layout_marginTop="680dp"
        android:layout_marginStart="152dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="27dp"
            android:layout_height="27.54dp"
            android:background="#F4F4F4"
            android:src="@drawable/facebook"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="86dp"
        android:layout_height="42dp"
        android:background="#F4F4F4"
        android:gravity="center"
        android:layout_marginTop="680dp"
        android:layout_marginStart="253dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="27dp"
            android:layout_height="27.54dp"
            android:background="#F4F4F4"
            android:src="@drawable/twitter"/>
    </LinearLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="748dp"
        android:layout_marginEnd="50dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="Have an account?"
        android:textColor="#888888"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="243dp"
        android:layout_marginTop="748dp"
        android:layout_marginEnd="50dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:letterSpacing="0.02"
        android:text="SIGN IN"
        android:textColor="#00B140"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>